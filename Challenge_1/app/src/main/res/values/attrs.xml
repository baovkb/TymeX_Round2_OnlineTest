<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircularKey">
        <attr name="text" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color"/>
        <attr name="android:textStyle" format="integer" />
    </declare-styleable>

    <declare-styleable name="StateIndicator">
        <attr name="loading_text" format="string" />
        <attr name="error_text" format="string" />
        <attr name="reload_text" format="string" />
        <attr name="success_text" format="string" />
        <attr name="animate_duration" format="integer" />
        <attr name="disappear_after_success_duration" format="integer"/>
    </declare-styleable>
</resources>